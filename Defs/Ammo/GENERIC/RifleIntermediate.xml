	<RecipeDef ParentName="AmmoRecipeBase">
		<defName>MakeAmmo_RifleIntermediate_FMJ</defName>
		<label>make intermediate rifle (FMJ) cartridge x200</label>
		<description>Craft 200 intermediate rifle (FMJ) cartridges.</description>
		<jobString>Making intermediate rifle (FMJ) cartridges.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>7</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>Steel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_RifleIntermediate_FMJ>200</Ammo_RifleIntermediate_FMJ>
		</products>
		<workAmount>1400</workAmount>
	</RecipeDef>

