		<statBases>
			<Mass>0.012</Mass>
			<Bulk>0.02</Bulk>
			<MarketValue>0.06</MarketValue>   
		</statBases>
		<tradeTags>
			<li>CE_AutoEnableTrade</li>
			<li>CE_AutoEnableCrafting</li>
		</tradeTags>
		<thingCategories>
			<li>AmmoRifleIntermediate</li>
		</thingCategories>
        <stackLimit>1000</stackLimit>
	</ThingDef>

