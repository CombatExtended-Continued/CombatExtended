		<statBases>
			<Mass>0.239</Mass>
			<Bulk>0.40</Bulk>
		</statBases>
		<tradeTags>
			<li>CE_AutoEnableTrade</li>
			<li>CE_AutoEnableCrafting_TableMachining</li>
		</tradeTags>
		<thingCategories>
			<li>AmmoLauncherGrenade</li>
		</thingCategories>
		<stackLimit>500</stackLimit>
		<cookOffFlashScale>100</cookOffFlashScale>
	</ThingDef>

