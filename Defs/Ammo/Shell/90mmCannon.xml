      </li>
      <li>
        <filter>
          <thingDefs>
            <li>FSX</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>2</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
        <li>FSX</li>
        <li>ComponentIndustrial</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Ammo_90mmCannonShell_HE>5</Ammo_90mmCannonShell_HE>
    </products>
  </RecipeDef>

  <RecipeDef ParentName="AmmoRecipeBase">
    <defName>MakeAmmo_90mmCannonShell_EMP</defName>
    <label>make 90mm EMP cannon shells x5</label>
    <description>Craft 5 90mm EMP cannon shells.</description>
    <jobString>Making 90mm EMP cannon shells.</jobString>
    <workAmount>27000</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Steel</li>
          </thingDefs>
        </filter>
        <count>192</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>ComponentIndustrial</li>
          </thingDefs>
        </filter>
        <count>13</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Steel</li>
