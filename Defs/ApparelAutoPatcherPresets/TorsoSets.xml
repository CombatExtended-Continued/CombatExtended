<?xml version="1.0" encoding="utf-8" ?>
<Defs>
    <CombatExtended.ApparelPatcherPresetDef>
        <defName>ArmorVestJacket</defName>
        <label>preset armored jacket</label>
        <Bulk>5</Bulk>
        <BulkWorn>5</BulkWorn>
        <Mass>4</Mass>
        <!-- If both are present just uses the appropiate curve, that is you can for example use static for sharp and curve for blunt -->
        <ArmorStaticSharp>8</ArmorStaticSharp>
        <ArmorStaticBlunt>12</ArmorStaticBlunt>
        <ArmorCurveSharp>
            <points>
                <li>0.40, 4</li>
                <li>0.80, 8</li>
            </points>
        </ArmorCurveSharp>
        <ArmorCurveBlunt>
            <points>
                <li>0.08, 1.5</li>
                <li>0.16, 3</li>
            </points>
        </ArmorCurveBlunt>
        <!-- IMPORTANT: This is SHARP armor range OR StuffEffectMultiplierArmor range-->
        <vanillaArmorRatingRange>0.4~1.5</vanillaArmorRatingRange>
        <neededLayers>
            <li>Shell</li>
        </neededLayers>
        <neededGroups>
            <li>Torso</li>
            <li>Neck</li>
            <li>Shoulders</li>
            <li>Arms</li>
        </neededGroups>
        <partialStats>
            <li>
                <ArmorRating_Sharp>0.75</ArmorRating_Sharp>
                <parts>
                   <li>Arm</li>
                </parts>
              </li>
        </partialStats>
    </CombatExtended.ApparelPatcherPresetDef>
</Defs>