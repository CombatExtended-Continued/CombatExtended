		<Compile Remove="CombatExtended\Comps\CompSquadBrain.cs" />
		<Compile Remove="CombatExtended\Jobs\JobGiver_SquadDuty.cs" />
		<Compile Remove="CombatExtended\Loadouts\HoldTrackerAssignment.cs" />
