		<Compile Remove="Projectile_FireTrail.cs" />
	</ItemGroup>
	<ItemGroup>
		<None Include="CombatExtended\ThinkNodes\ThinkNode_ConditionalNoJob.cs" />
	</ItemGroup>
	<ItemGroup>
	  <Reference Include="CombatExtendedLoader">
	    <HintPath>..\..\Assemblies\CombatExtendedLoader.dll</HintPath>
	    <Private>False</Private>
	  </Reference>
	  
	  <Reference Include="Rimatomics">
