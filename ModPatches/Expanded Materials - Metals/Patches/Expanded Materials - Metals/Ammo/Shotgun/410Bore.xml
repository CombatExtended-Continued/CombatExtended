<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<!-- Buckshot -->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/RecipeDef[defName="MakeAmmo_410Bore_Buck"]/ingredients</xpath>
		<value>
			<ingredients>
				<li>
					<filter>
						<thingDefs>
							<li>VMEu_Bronze</li>
							<li>Steel</li>
						</thingDefs>
					</filter>
					<count>1</count>
				</li>
				<li>
					<filter>
						<thingDefs>
							<li>Chemfuel</li>
						</thingDefs>
					</filter>
					<count>1</count>
				</li>
				<li>
					<filter>
						<thingDefs>
							<li>VMEu_Lead</li>
						</thingDefs>
					</filter>
					<count>3</count>
				</li>
			</ingredients>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/RecipeDef[defName="MakeAmmo_410Bore_Buck"]/fixedIngredientFilter</xpath>
		<value>
			<fixedIngredientFilter>
				<thingDefs>
					<li>VMEu_Bronze</li>
					<li>Steel</li>
				</thingDefs>
			</fixedIngredientFilter>
		</value>
	</Operation>
</Patch>