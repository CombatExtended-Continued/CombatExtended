	<ThingDef Class="CombatExtended.AmmoDef" ParentName="6x24mmPsychicBase">
		<defName>Ammo_6x24mmPsychic_Neuro</defName>
		<label>6x24mm psychic (Neuro)</label>
		<graphicData>
			<texPath>ThirdParty/PsyBlasters/MediumNeuro</texPath>
