			<Ammo_12GaugePsychic_Neuro>Bullet_12GaugePsychic_Neuro</Ammo_12GaugePsychic_Neuro>
		</ammoTypes>
		<!--<similarTo>AmmoSet_PsychicShotgun</similarTo>-->
