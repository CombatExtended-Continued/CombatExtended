	<!-- ==================== AmmoSet ========================== -->

	<CombatExtended.AmmoSetDef>
		<defName>AmmoSet_12GaugePsychic</defName>
		<label>12 Gauge Psychic</label>
		<ammoTypes>
			<Ammo_12GaugePsychic>Bullet_12GaugePsychic</Ammo_12GaugePsychic>
