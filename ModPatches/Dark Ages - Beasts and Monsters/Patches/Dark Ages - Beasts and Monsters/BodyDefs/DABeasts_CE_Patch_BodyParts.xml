<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationAdd">
		<xpath>Defs/BodyPartDef[defName="DA_InsectTail" or defName="DA_Pincer"]</xpath>
		<value>
			<bleedRate>1.5</bleedRate>
		</value>
	</Operation>

	<Operation Class="PatchOperationReplace">
		<xpath>Defs/BodyPartDef[defName="DA_Pincer"]/hitPoints</xpath>
		<value>
			<hitPoints>20</hitPoints>
		</value>
	</Operation>

</Patch>