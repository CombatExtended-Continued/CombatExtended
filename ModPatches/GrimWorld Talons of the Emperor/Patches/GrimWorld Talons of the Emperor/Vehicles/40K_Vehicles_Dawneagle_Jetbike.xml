				<li Class="PatchOperationReplace">
					<xpath>Defs/Vehicles.VehicleDef[defName="GW_DawnEagleBike"]/components/li[key="RightArmorPlating"]/armor/ArmorRating_Sharp</xpath>
					<value>
						<ArmorRating_Sharp>22</ArmorRating_Sharp>
					</value>
				</li>

