<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<!--Adjust palisade embrasure fill.-->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="DankPyon_EmbPalisade"]/fillPercent</xpath>
		<value>
			<fillPercent>0.70</fillPercent>
		</value>
	</Operation>

	<!--Adjust castle embrasure fill.-->
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[defName="DankPyon_CastleWallEmbrasures"]/fillPercent</xpath>
		<value>
			<fillPercent>0.70</fillPercent>
		</value>
	</Operation>

</Patch>