						<xpath>Defs/BodyDef[defName="AA_WormWithArmor"]/corePart/parts/li[def="SnakeHead"]/groups</xpath>
						<value>
							<li>CoveredByNaturalArmor</li>
						</value>
					</match>
				</li>

				<li Class="PatchOperationConditional">
					<xpath>Defs/BodyDef[defName="AA_WormWithArmor"]/corePart/parts/li[def="SnakeHead"]/parts/li[def="Jaw"]/groups</xpath>
					<nomatch Class="PatchOperationAdd">
						<xpath>Defs/BodyDef[defName="AA_WormWithArmor"]/corePart/parts/li[def="SnakeHead"]/parts/li[def="Jaw"]</xpath>
						<value>
							<groups>
								<li>CoveredByNaturalArmor</li>
							</groups>
						</value>
					</nomatch>
					<match Class="PatchOperationAdd">
						<xpath>Defs/BodyDef[defName="AA_WormWithArmor"]/corePart/parts/li[def="SnakeHead"]/parts/li[def="Jaw"]/groups</xpath>
						<value>
							<li>CoveredByNaturalArmor</li>
