<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Animals</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>

				<!-- ====== WormWithArmor ====== -->

				<li Class="PatchOperationConditional">
					<xpath>Defs/BodyDef[defName="AA_WormWithArmor"]/corePart/groups</xpath>
					<nomatch Class="PatchOperationAdd">
						<xpath>Defs/BodyDef[defName="AA_WormWithArmor"]/corePart</xpath>
						<value>
							<groups>
								<li>CoveredByNaturalArmor</li>
							</groups>
						</value>
					</nomatch>
					<match Class="PatchOperationAdd">
						<xpath>Defs/BodyDef[defName="AA_WormWithArmor"]/corePart/groups</xpath>
						<value>
							<li>CoveredByNaturalArmor</li>
						</value>
					</match>
				</li>

				<li Class="PatchOperationConditional">
					<xpath>Defs/BodyDef[defName="AA_WormWithArmor"]/corePart/parts/li[def="AA_FirstWormSegment" or def="AA_SecondWormSegment" or def="AA_ThirdWormSegment" or def="AA_FourthWormSegment"]/groups</xpath>
					<nomatch Class="PatchOperationAdd">
						<xpath>Defs/BodyDef[defName="AA_WormWithArmor"]/corePart/parts/li[def="AA_FirstWormSegment" or def="AA_SecondWormSegment" or def="AA_ThirdWormSegment" or def="AA_FourthWormSegment"]</xpath>
						<value>
							<groups>
								<li>CoveredByNaturalArmor</li>
							</groups>
						</value>
					</nomatch>
					<match Class="PatchOperationAdd">
						<xpath>Defs/BodyDef[defName="AA_WormWithArmor"]/corePart/parts/li[def="AA_FirstWormSegment" or def="AA_SecondWormSegment" or def="AA_ThirdWormSegment" or def="AA_FourthWormSegment"]/groups</xpath>
						<value>
							<li>CoveredByNaturalArmor</li>
						</value>
					</match>
				</li>

				<li Class="PatchOperationConditional">
					<xpath>Defs/BodyDef[defName="AA_WormWithArmor"]/corePart/parts/li[def="SnakeHead"]/groups</xpath>
					<nomatch Class="PatchOperationAdd">
						<xpath>Defs/BodyDef[defName="AA_WormWithArmor"]/corePart/parts/li[def="SnakeHead"]</xpath>
						<value>
							<groups>
								<li>CoveredByNaturalArmor</li>
							</groups>
						</value>
					</nomatch>
