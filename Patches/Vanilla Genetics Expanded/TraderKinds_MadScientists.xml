<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Genetics Expanded</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<!-- Caravan Trader -->

				<li Class="PatchOperationAdd">
					<xpath>Defs/TraderKindDef[defName="GR_Outlander_CaravanTrader"]/stockGenerators</xpath>
					<value>
						<li Class="StockGenerator_SingleDef">
							<thingDef>FSX</thingDef>
							<countRange>
								<min>25</min>
								<max>100</max>
							</countRange>
						</li>
						<li Class="StockGenerator_SingleDef">
							<thingDef>Prometheum</thingDef>
							<countRange>
								<min>15</min>
								<max>100</max>
							</countRange>
						</li>
					</value>
				</li>

				<!-- Orbital Trader -->

				<li Class="PatchOperationAdd">
					<xpath>Defs/TraderKindDef[defName="GR_OrbitalTrader"]/stockGenerators</xpath>
					<value>
						<li Class="StockGenerator_SingleDef">
							<thingDef>FSX</thingDef>
							<countRange>
								<min>50</min>
								<max>200</max>
							</countRange>
						</li>
						<li Class="StockGenerator_SingleDef">
							<thingDef>Prometheum</thingDef>
							<countRange>
								<min>50</min>
								<max>200</max>
							</countRange>
						</li>
					</value>
				</li>

			</operations>
		</match>
	</Operation>
</Patch>