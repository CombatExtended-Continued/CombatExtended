<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Genetics Expanded</li>
		</mods>

		<match Class="PatchOperationSequence">
		<operations>

		<!--GR_FeatherdustJoint -->

		<li Class="PatchOperationAdd">
			<xpath>Defs/ThingDef[defName="GR_FeatherdustJoint"]/statBases</xpath>
			<value>
				<Bulk>0.12</Bulk>
			</value>
		</li>

		<!--GR_ElectroEgg -->

		<li Class="PatchOperationAdd">
			<xpath>Defs/ThingDef[defName="GR_ElectroEgg"]/statBases</xpath>
			<value>
				<Bulk>0.10</Bulk>
			</value>
		</li>

		<!--GR_PoisonAmpoule -->

		<li Class="PatchOperationAdd">
			<xpath>Defs/ThingDef[defName="GR_PoisonAmpoule"]/statBases</xpath>
			<value>
				<Bulk>0.05</Bulk>
			</value>
		</li>

		</operations>
		</match>
	</Operation>
</Patch>