<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Medical System Expansion - Revived</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<!-- ==== Bases ==== -->
				<!-- Simple -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/HediffDef[@Name="MSE_AddedBodyPartSimpleBase"]/stages/li/statOffsets</xpath>
					<value>
						<statOffsets>
							<ArmorRating_Blunt>0.138</ArmorRating_Blunt>
							<ArmorRating_Sharp>0.075</ArmorRating_Sharp>
						</statOffsets>
					</value>
				</li>
				<!-- Bionic -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/HediffDef[@Name="MSE_AddedBodyPartBionicBase"]/stages/li/statOffsets</xpath>
					<value>
						<statOffsets>
							<ArmorRating_Blunt>0.39</ArmorRating_Blunt>
							<ArmorRating_Sharp>0.212</ArmorRating_Sharp>
							<ArmorRating_Heat>0.106</ArmorRating_Heat>
						</statOffsets>
					</value>
				</li>
				<!-- Archotech -->
				<li Class="PatchOperationReplace">
					<xpath>Defs/HediffDef[@Name="MSE_AddedBodyPartArchotechBase"]/stages/li/statOffsets</xpath>
					<value>
						<statOffsets>
							<ArmorRating_Blunt>1.102</ArmorRating_Blunt>
							<ArmorRating_Sharp>0.6</ArmorRating_Sharp>
							<ArmorRating_Heat>0.3</ArmorRating_Heat>
						</statOffsets>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>