  <!-- ========== Add armor coverage ========== -->

      <li Class="PatchOperationAdd">
        <xpath>
          /Defs/BodyDef[defName="Cyborg"]//*[
          def="CyborgTorso" or 
          def="CyborgNeck" or 
          def="Head" or 
          def="Jaw" or 
          def="CyborgEar" or
          def="CyborgNose" or
          def="CyborgShoulder" or
          def="Shoulder" or
          def="CyborgArm" or
          def="Arm" or
          def="CyborgBladeFist" or
          def="CyborgHand" or
          def="Hand" or
          def="CyborgFoot" or
          def="CyborgLeg"]/groups
        </xpath>
        <value>
          <li>CoveredByNaturalArmor</li>
        </value>
      </li>

      <li Class="PatchOperationAdd">
        <xpath>
          /Defs/BodyDef[defName="Cyborg"]//*[
          def="CyborgJaw" or 
          def="CyborgNose" or
          def="CyborgSightSensor" or
          def="CyborgEye"]/groups
        </xpath>
        <value>
          <li>OutsideSquishy</li>
        </value>
      </li>
