<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Races Expanded - Sanguophage</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>Defs/ThingDef[defName="VRE_HemogenPack_Animal" or defName="VRE_HemogenPack_Corpse" or defName="VRE_HemogenPack_Sanguophage"]/statBases</xpath>
					<value>
						<Bulk>0.25</Bulk>
					</value>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>