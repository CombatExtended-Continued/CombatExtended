						<li>LeftShoulder</li>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/BodyDef[defName="ATR_AndroidBody"]/corePart/parts/li[customLabel="right shoulder"]/groups</xpath>
