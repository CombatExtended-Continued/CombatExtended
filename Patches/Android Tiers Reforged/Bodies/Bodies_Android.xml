						<li>CoveredByNaturalArmor</li>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/BodyDef[defName="ATR_AndroidBody"]/corePart/parts/li[def="ATR_MechanicalNeck"]/parts/li[def="ATR_MechanicalHead"]/groups</xpath>
