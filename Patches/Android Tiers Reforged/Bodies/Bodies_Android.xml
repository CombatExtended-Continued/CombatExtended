					<value>
						<li>LeftArm</li>
					</value>
				</li>

				<li Class="PatchOperationAdd">
					<xpath>Defs/BodyDef[defName="ATR_AndroidBody"]/corePart/parts/li[def="ATR_MechanicalShoulder"]/parts/li[customLabel="right arm"]/groups</xpath>
