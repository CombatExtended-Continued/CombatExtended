      <li>
        <filter>
          <thingDefs>
            <li>Ammo_30x64mmFuel_Incendiary</li>
          </thingDefs>
        </filter>
        <count>200</count>
      </li> 
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Ammo_30x64mmFuel_Incendiary</li>	  
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Ammo_30x64mmFuel_Incendiary_Punched>50</Ammo_30x64mmFuel_Incendiary_Punched>
