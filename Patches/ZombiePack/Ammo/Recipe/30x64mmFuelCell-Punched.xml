      <thingDefs>
        <li>Ammo_30x64mmFuel_Thermobaric</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Ammo_30x64mmFuel_Thermobaric_Punched>50</Ammo_30x64mmFuel_Thermobaric_Punched>
    </products>
    <workAmount>10400</workAmount>		
