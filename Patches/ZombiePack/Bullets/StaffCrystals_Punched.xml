<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Call of Duty: Zombies Pack</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

		<li Class="PatchOperationAdd">
			<xpath>Defs</xpath>
			<value>

<!--Fire-->

	<ThingDef ParentName="PunchedElementalStaffBullet">
		<defName>Bullet_Elemental_Staff_Fire_Punched</defName>
		<label>Empowered Fire Projectile</label>
		<graphicData>
			<color>(255,48,48)</color>
		</graphicData>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>1</damageAmountBase>
			<pelletCount>3</pelletCount>
			<armorPenetrationSharp>1</armorPenetrationSharp>
			<armorPenetrationBlunt>1</armorPenetrationBlunt>
			<spreadMult>30</spreadMult>
		</projectile>
		<comps>
			<li Class="CombatExtended.CompProperties_ExplosiveCE">
				<damageAmountBase>45</damageAmountBase>
				<explosiveDamageType>Bomb</explosiveDamageType>
				<explosiveRadius>1.9</explosiveRadius>
				<explosionSound>MortarBomb_Explode</explosionSound>
				<applyDamageToExplosionCellsNeighbors>true</applyDamageToExplosionCellsNeighbors>
			</li>
		</comps>
	</ThingDef>
<!--Wind-->

	<ThingDef ParentName="PunchedElementalStaffBullet">
		<defName>Bullet_Elemental_Staff_Wind_Punched</defName>
		<label>Empowered Wind Projectile</label>
		<graphicData>
			<color>(0, 255, 0)</color>
		</graphicData>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageAmountBase>1</damageAmountBase>
			<armorPenetrationSharp>1</armorPenetrationSharp>
			<armorPenetrationBlunt>1</armorPenetrationBlunt>
		</projectile>
	</ThingDef>
<!--Ice-->

	<ThingDef ParentName="PunchedElementalStaffBullet">
		<defName>Bullet_Elemental_Staff_Ice_Punched</defName>
		<label>Empowered Ice Projectile</label>
		<graphicData>
			<color>(162,210,223)</color>
		</graphicData>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageDef>CodZP_Frostbite</damageDef>
			<damageAmountBase>1</damageAmountBase>
			<armorPenetrationSharp>1</armorPenetrationSharp>
			<armorPenetrationBlunt>1</armorPenetrationBlunt>
		</projectile>
	</ThingDef>
<!--Lightning-->

	<ThingDef ParentName="PunchedElementalStaffBullet">
		<defName>Bullet_Elemental_Staff_Lightning_Punched</defName>
		<label>Empowered Lightning Projectile</label>
		<graphicData>
			<shaderType>TransparentPostLight</shaderType>
			<color>(113, 4, 255)</color>
		</graphicData>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<damageDef>Electrical</damageDef>
			<pelletCount>2</pelletCount>
			<damageAmountBase>1</damageAmountBase>
			<armorPenetrationSharp>1</armorPenetrationSharp>
			<armorPenetrationBlunt>1</armorPenetrationBlunt>
			<spreadMult>10</spreadMult>
		</projectile>
	</ThingDef>

			</value>
			</li>

			</operations>
		</match>
	</Operation>

</Patch>