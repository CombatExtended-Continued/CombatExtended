<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Call of Duty: Pack A Punch</li>
        	<li>Call of Duty: Zombies Pack</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

		<li Class="PatchOperationAdd">
			<xpath>Defs</xpath>
			<value>

 <!--Setup-->

	<ThingCategoryDef>
		<defName>AmmoPunched</defName>
		<label>Pack a Punched</label>
		<parent>Ammo</parent>
	</ThingCategoryDef>

  <RecipeDef Name="AmmoRecipePunched" ParentName="AmmoRecipeBase" Abstract="true">
  	<researchPrerequisite>CodZP_PackAPunch</researchPrerequisite>
  </RecipeDef>

 <!--Extensions-->
	<ThingCategoryDef>
      <defName>AmmoAdvanced_Punched</defName>
      <label>advanced</label>
      <parent>AmmoPunched</parent>
      <iconPath>UI/Icons/ThingCategories/AmmoAdvanced</iconPath>
    </ThingCategoryDef>

	<ThingCategoryDef>
      <defName>AmmoPistols_Punched</defName>
      <label>pistol</label>
      <parent>AmmoPunched</parent>
      <iconPath>UI/Icons/ThingCategories/AmmoPistols</iconPath>
    </ThingCategoryDef>

    <ThingCategoryDef>
      <defName>AmmoRifles_Punched</defName>
      <label>rifle</label>
      <parent>AmmoPunched</parent>
      <iconPath>UI/Icons/ThingCategories/AmmoRifles</iconPath>
    </ThingCategoryDef>

    <ThingCategoryDef>
      <defName>AmmoShotguns_Punched</defName>
      <label>shotgun</label>
      <parent>AmmoPunched</parent>
      <iconPath>UI/Icons/ThingCategories/AmmoShotguns</iconPath>
    </ThingCategoryDef>

			</value>
			</li>

			</operations>
		</match>
	</Operation>

</Patch>