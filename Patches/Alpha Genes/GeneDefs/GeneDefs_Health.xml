<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Alpha Genes</li>
		</mods>

			<match Class="PatchOperationSequence">
				<operations>

					<li Class="PatchOperationAdd">
						<xpath>Defs/GeneDef[defName="AG_PackMule"]/statFactors</xpath>
						<value>
							<CarryWeight>1.5</CarryWeight>
						</value>
					</li>

					<li Class="PatchOperationAdd">
						<xpath>Defs/GeneDef[defName="AG_FrailBack"]/statFactors</xpath>
						<value>
							<CarryWeight>0.5</CarryWeight>
						</value>
					</li>

				</operations>
			</match>
	</Operation>

</Patch>
