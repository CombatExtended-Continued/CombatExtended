<?xml version="1.0" encoding="utf-8" ?>
<Patch>

	<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Vanilla Factions Expanded - Vikings</li>
    </mods>
		<match Class="PatchOperationSequence">
		<operations>

			<li Class="PatchOperationReplace">
				<xpath>Defs/ScenarioDef[defName="VFEV_NewClan"]/scenario/parts/li[thingDef="VFE_Harpoon" and @Class="ScenPart_StartingThing_Defined"]/count</xpath>
				<value>
					<count>20</count>
				</value>
			</li>	

			<li Class="PatchOperationAdd">
				<xpath>Defs/ScenarioDef[defName="VFEV_NewClan"]/scenario/parts</xpath>
				<value>
					<li Class="ScenPart_StartingThing_Defined">
					  <def>StartingThing_Defined</def>
					  <thingDef>Ammo_Arrow_Steel</thingDef>
					  <count>100</count>
					</li>
				</value>
			</li>

		</operations>
		</match>
	</Operation>
</Patch>    