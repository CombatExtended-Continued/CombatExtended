<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Call of Duty: Zombies Pack</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>

						<RecipeDef Name="AmmoRecipePunched" ParentName="AmmoRecipeBase" Abstract="true">
							<researchPrerequisite>
								<li>CodZP_PackAPunch</li>
							</researchPrerequisite>
						</RecipeDef>

						<ThingCategoryDef>
							<defName>AmmoPunched</defName>
							<label>Pack a Punched</label>
							<parent>Ammo</parent>
						</ThingCategoryDef>

						<ThingCategoryDef>
							<defName>AmmoAdvanced_Punched</defName>
							<label>advanced</label>
							<parent>AmmoPunched</parent>
						</ThingCategoryDef>

						<ThingCategoryDef>
							<defName>AmmoPistols_Punched</defName>
							<label>pistol</label>
							<parent>AmmoPunched</parent>
						</ThingCategoryDef>

						<ThingCategoryDef>
							<defName>AmmoRifles_Punched</defName>
							<label>rifle</label>
							<parent>AmmoPunched</parent>
						</ThingCategoryDef>

						<ThingCategoryDef>
							<defName>AmmoShotguns_Punched</defName>
							<label>shotgun</label>
							<parent>AmmoPunched</parent>
						</ThingCategoryDef>

					</value>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>