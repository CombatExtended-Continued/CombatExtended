<?xml version="1.0" encoding="utf-8"?>
<Patch>

	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Call of Duty: Zombies Pack</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>

				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>

						<ThingDef ParentName="BaseHellsRetriever">
							<defName>Bullet_Hells_Retriever_Punched</defName>
							<label>Hells Redeemer</label>
							<graphicData>
								<texPath>Things/Projectile/CODZP_Hell_Redeemer</texPath>
							</graphicData>
							<projectile Class="CombatExtended.ProjectilePropertiesCE">
								<speed>165</speed>
								<damageAmountBase>80</damageAmountBase>
								<armorPenetrationSharp>40</armorPenetrationSharp>
								<armorPenetrationBlunt>40</armorPenetrationBlunt>
							</projectile>
						</ThingDef>
					</value>
				</li>

			</operations>
		</match>
	</Operation>

</Patch>