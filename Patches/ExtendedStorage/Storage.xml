<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Extended Storage</li>
		</mods>
		<match Class="PatchOperationSequence">
		<operations>
	    <li Class="PatchOperationAdd">
	    	<xpath>Defs/ThingDef[defName="Storage_WoodenPallet"]/building/fixedStorageSettings/filter/categories</xpath>
	    	<value>
	    		<li>Ammo</li>
	    	</value>
	    </li>
	    <li Class="PatchOperationRemove">
	    	<xpath>Defs/ThingDef[defName="Storage_WoodenPallet"]/building/fixedStorageSettings/filter/thingDefs/li[.="MortarShell"]</xpath>
	    </li>
	  </operations>
	  </match>
	</Operation>
</Patch>

