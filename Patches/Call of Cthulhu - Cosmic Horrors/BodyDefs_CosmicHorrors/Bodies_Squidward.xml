<?xml version="1.0" encoding="UTF-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Call of Cthulhu - Cosmic Horrors</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>/Defs/BodyDef[defName="ROM_CthulhidBody"]//*[
            def="Torso" or 
            def="Neck" or 
            def="Shoulder" or 
            def="Arm" or 
            def="Hand" or 
            def="ROM_RightWing" or 
            def="ROM_LeftWing" or 
            def="Leg"]/groups</xpath>
					<value>
						<li>CoveredByNaturalArmor</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>