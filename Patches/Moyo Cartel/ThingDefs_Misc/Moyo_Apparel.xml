				<xpath>Defs/ThingDef[defName="Moyo_Cartelsuit"]/apparel/bodyPartGroups</xpath>
				<value>
					<li>Hands</li>
					<li>Feet</li>
				</value>
			</li>
			
			<!--Evasuit-->
			
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[defName="Moyo_CartelStrikerHood"]/statBases</xpath>
