			<!--Strike Helmet-->
			
			<li Class="PatchOperationAdd">
				<xpath>Defs/ThingDef[defName="Moyo_CShelmet"]/statBases</xpath>
				<value>
					<Bulk>6</Bulk>
					<WornBulk>1</WornBulk>
					<NightVisionEfficiency_Apparel>0.75</NightVisionEfficiency_Apparel> 
