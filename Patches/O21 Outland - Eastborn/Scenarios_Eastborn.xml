      <!-- ========== Starting Things ========== -->
      <li Class="PatchOperationAdd">
        <xpath>/Defs/ScenarioDef[defName="Outland_EastbornExpedition"]/scenario/parts</xpath>
        <value>
          <li Class="ScenPart_StartingThing_Defined">
            <def>StartingThing_Defined</def>
            <thingDef>Ammo_Arrow_Steel</thingDef>
            <count>100</count>
          </li>
        </value>
      </li>

      </operations>
    </match>
  </Operation>
