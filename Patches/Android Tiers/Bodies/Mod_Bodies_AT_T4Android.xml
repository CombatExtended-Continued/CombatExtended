  <li Class="PatchOperationReplace">
    <xpath>Defs/BodyPartDef[defName="LeftMechanicalLeg" or defName="RightMechanicalLeg"]/hitPoints</xpath>
    <value>
      <hitPoints>40</hitPoints>
    </value>
