<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Marilyn the Mincho Worshipper Witch</li>
		</mods>
		<match Class="PatchOperationSequence">
		<operations>
					
			<!--Arm Groups-->
			
			<li Class="PatchOperationAdd">
				<xpath>
				/Defs/BodyDef[defName="MinchoWitch"]//*[
				customLabel="left shoulder"
				]/groups
				</xpath>
				<value>
				<li>LeftShoulder</li>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>
				/Defs/BodyDef[defName="MinchoWitch"]//*[
				customLabel="right shoulder"
				]/groups
				</xpath>
				<value>
				<li>RightShoulder</li>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>
				/Defs/BodyDef[defName="MinchoWitch"]//*[
				customLabel="left arm"
				]/groups
				</xpath>
				<value>
				<li>LeftArm</li>
				</value>
			</li>
			
			<li Class="PatchOperationAdd">
				<xpath>
				/Defs/BodyDef[defName="MinchoWitch"]//*[
				customLabel="right arm"
				]/groups
				</xpath>
				<value>
				<li>RightArm</li>
				</value>
			</li>
		
			<!--Natural Armor-->

			<li Class="PatchOperationAdd">
				<xpath>
				/Defs/BodyDef[defName="MinchoWitch"]//*[
				def="Torso" or 
				def="Neck" or 
				def="Head" or 
				def="Nose" or 
				def="Jaw" or 
				def="Shoulder" or 
				def="Arm" or 
				def="Hand" or 
				def="Finger" or 
				def="Waist" or 
				def="Leg" or 
				def="Foot" or 
				def="Toe"
				]/groups
				</xpath>
				<value>
				<li>CoveredByNaturalArmor</li>
				</value>
			</li>

		</operations>
		</match>
	</Operation>
</Patch>