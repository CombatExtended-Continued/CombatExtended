	<RecipeDef ParentName="AmmoRecipeBase">
		<defName>MakeAmmo_18mmSmart_FMJ</defName>
		<label>make 18mm Smart (FMJ) rocket x200</label>
		<description>Craft 500 18mm Smart (FMJ) rockets.</description>
		<jobString>Making 18mm Smart (FMJ) rockets.</jobString>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>Steel</li>
					</thingDefs>
				</filter>
				<count>22</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>ComponentIndustrial</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Prometheum</li>
					</thingDefs>
				</filter>
				<count>2</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
		 	<thingDefs>
    	 		<li>Steel</li>
    	 		<li>Prometheum</li>
    	 		<li>ComponentIndustrial</li>
    	 	</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Ammo_18mmSmart_FMJ>200</Ammo_18mmSmart_FMJ>
		</products>
		<workAmount>3400</workAmount>
	</RecipeDef>

