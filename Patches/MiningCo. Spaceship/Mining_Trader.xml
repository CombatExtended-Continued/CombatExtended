<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
		<li>MiningCo. Spaceship</li>
		</mods>
		<match Class="PatchOperationSequence">
		<operations>

			<!-- ==========  ========== -->

			<li Class="PatchOperationAdd">
				<xpath>Defs/TraderKindDef[defName="SpaceshipCargoPeriodicSupply"]/stockGenerators</xpath>
				<value>
      				<li Class="StockGenerator_SingleDef">
        				<thingDef>FSX</thingDef>
        				<countRange>
          				<min>10</min>
          				<max>25</max>
        				</countRange>
      				</li>
      				<li Class="StockGenerator_SingleDef">
        				<thingDef>Prometheum</thingDef>
        				<countRange>
          				<min>10</min>
          				<max>25</max>
        				</countRange>
      				</li>
					<li Class="StockGenerator_Tag">
						<tradeTag>CE_Ammo</tradeTag>
						<countRange>
							<min>500</min>
							<max>1000</max>
						</countRange>
						<thingDefCountRange>
							<min>2</min>
							<max>6</max>
						</thingDefCountRange>
					</li>
				</value>
			</li>

			<li Class="PatchOperationAdd">
				<xpath>Defs/TraderKindDef[defName="SpaceshipCargoRequestedSupply" or defName="SpaceshipCargoDispatcher"]/stockGenerators</xpath>
				<value>
      				<li Class="StockGenerator_SingleDef">
        				<thingDef>FSX</thingDef>
        				<countRange>
          				<min>15</min>
          				<max>45</max>
        				</countRange>
      				</li>
      				<li Class="StockGenerator_SingleDef">
        				<thingDef>Prometheum</thingDef>
        				<countRange>
          				<min>15</min>
          				<max>45</max>
        				</countRange>
      				</li>
					<li Class="StockGenerator_Tag">
						<tradeTag>CE_Ammo</tradeTag>
						<countRange>
          							<min>1000</min>
          							<max>3000</max>
						</countRange>
						<price>Cheap</price>
						<thingDefCountRange>
          						<min>5</min>
          						<max>12</max>
						</thingDefCountRange>
					</li>
				</value>
			</li>

		<li Class="PatchOperationRemove">
				<xpath>Defs/TraderKindDef[defName="SpaceshipCargoDispatcher"]/stockGenerators/li[thingDef="Gun_Minigun"]</xpath>
			</li>

			<li Class="PatchOperationAdd">
				<xpath>Defs/TraderKindDef[defName="SpaceshipCargoDispatcher"]/stockGenerators</xpath>
				<value>
      					<li Class="StockGenerator_SingleDef">
        					<thingDef>Gun_LMG</thingDef>
        					<countRange>
          					<min>-1</min>
          					<max>1</max>
        					</countRange>
      					</li>
				</value>
			</li>

			<li Class="PatchOperationAdd">
				<xpath>Defs/TraderKindDef[defName="SpaceshipCargoDamaged"]/stockGenerators</xpath>
				<value>
					<li Class="StockGenerator_Tag">
						<tradeTag>CE_Ammo</tradeTag>
        						<price>Cheap</price>
						<countRange>
          							<min>100</min>
          							<max>300</max>
						</countRange>
						<thingDefCountRange>
          						<min>1</min>
          						<max>3</max>
						</thingDefCountRange>
					</li>
				</value>
			</li>

			<li Class="PatchOperationAdd">
				<xpath>Defs/TraderKindDef[defName="SpaceshipCargoMedical"]/stockGenerators</xpath>
				<value>
					<li Class="StockGenerator_Tag">
						<tradeTag>CE_Ammo</tradeTag>
						<countRange>
							<min>30</min>
							<max>60</max>
						</countRange>
						<thingDefCountRange>
							<min>-1</min>
							<max>1</max>
						</thingDefCountRange>
					</li>
				</value>
			</li>


		</operations>
		</match>
	</Operation>
</Patch>