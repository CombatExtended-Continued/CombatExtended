<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>ADE Advanced Turrets +</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationRemove">
					<xpath>Defs/RecipeDef[
						defName="Make_Ammo_x_AHST_CannonShell" or
						defName="Make_Ammo_xx_AHST_CannonShell"
						] </xpath>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/ThingDef[
						defName="AHST_Ammo"
						] </xpath>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>