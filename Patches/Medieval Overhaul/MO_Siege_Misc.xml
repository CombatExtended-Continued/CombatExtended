<?xml version="1.0" encoding="utf-8" ?>
<Patch>

   <Operation Class="PatchOperationFindMod">
      <mods>
         <li>Medieval Overhaul</li>
      </mods>
      <match Class="PatchOperationSequence">
         <operations>

            <!--Adjust palisade embrasure fill.-->
            <li Class="PatchOperationReplace">
               <xpath>Defs/ThingDef[defName="DankPyon_EmbPalisade"]/fillPercent</xpath>
               <value>
                  <fillPercent>0.70</fillPercent>
               </value>
            </li>

            <!--Adjust castle embrasure fill.-->
            <li Class="PatchOperationReplace">
               <xpath>Defs/ThingDef[defName="DankPyon_CastleWallEmbrasures"]/fillPercent</xpath>
               <value>
                  <fillPercent>0.70</fillPercent>
               </value>
            </li>

         </operations>
      </match>
   </Operation>

</Patch>